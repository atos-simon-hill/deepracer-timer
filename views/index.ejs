<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>deepracer-timer</title>
    <link rel="stylesheet" href="./timer.css">
</head>
<body>
    <nav class="controls">
        <a href="#" id="btn_start" class="button" onClick="stopwatch.start();">Start</a>
        <a href="#" id="btn_pause" class="button" onClick="stopwatch.pause();">Pause</a>
        <a href="#" id="btn_passed" class="button" onClick="stopwatch.passed();">Passed</a>
        <a href="#" id="btn_reset" class="button" onClick="stopwatch.reset();">Reset</a>
        <a href="#" id="btn_clear" class="button" onClick="stopwatch.clear();">Clear</a>
    </nav>
    <div class="stopwatch"></div>
    <ul class="results"></ul>
</body>
</html>

<script src="/socket.io/socket.io.js"></script>
<script src="./timer.js"></script>
